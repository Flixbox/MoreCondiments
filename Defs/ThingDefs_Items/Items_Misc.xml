<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef ParentName="MealCooked">
        <defName>MC_MeatCooked</defName>
        <label>cooked meat</label>
        <description>Some chunks of meat that were cooked in broth. A simplistic and filling meal.</description>
        <graphicData>
            <texPath>Things/Item/Resource/Kibble</texPath>
            <color>(170,30,30)</color>
            <drawSize>0.8</drawSize>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <statBases>
            <MarketValue>15</MarketValue>
            <Nutrition>0.9</Nutrition>
        </statBases>
        <ingestible>
            <preferability>MealSimple</preferability>
            <ingestEffect>EatVegetarian</ingestEffect>
            <ingestSound>Meal_Eat</ingestSound>
            <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
            <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
        </ingestible>
    </ThingDef>

</Defs>